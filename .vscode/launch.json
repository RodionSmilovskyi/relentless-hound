    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug train",
            "type": "debugpy",
            "request": "launch",
            "program": "package/train",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
                "WRAPT_DISABLE_EXTENSIONS": "true"
            },
            "args": [
                "--total-timesteps",
                "1000000",
                "--learn-rate",
                "0.0001",
                "--entropy",
                "0.5",
                "--timesteps-per-batch",
                "10000",
                "--num-minibatches",
                "5",
                "--noise-ann",
                "0.6-0.2"
            ]
        }
    ]
}