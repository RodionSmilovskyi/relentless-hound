    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug train",
            "type": "debugpy",
            "request": "launch",
            "program": "package/train",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
                "WRAPT_DISABLE_EXTENSIONS": "true"
            },
            "args": [
                "--total-timesteps",
                "500000",
                "--learn-rate",
                "0.0005",
                "--entropy",
                "0.5",
                "--timesteps-per-batch",
                "10000",
                "--num-minibatches",
                "4",
                "--use-noise",
                "--use-lr-ann"
            ]
        }
    ]
}